{
  "name": "timgolz-shopping-cart",
  "proxy": "http://localhost:5000",
  "homepage": "https://timgolz-shopping-cart.herokuapp.com/",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^12.8.3",
    "body-parser": "^1.19.0",
    "concurrently": "^6.1.0",
    "express": "^4.17.1",
    "http-proxy-middleware": "^2.0.0",
    "mongoose": "^5.12.7",
    "nodemon": "^2.0.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-modal": "^3.13.1",
    "react-redux": "^7.2.4",
    "react-reveal": "^1.2.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "redux": "^4.1.0",
    "redux-thunk": "^2.3.0",
    "shortid": "^2.2.16",
    "web-vitals": "^1.1.2"
  },
  "engines": {
    "node": "12.18.3"
  },
  "scripts": {
    "timgolz-shopping-cart-install": "npm install --prefix timgolz-shopping-cart",
    "start": "node server.js",
    "server": "nodemon server.js",
    "timgolz-shopping-cart": "npm start --prefix timgolz-shopping-cart",
    "dev": "concurrently \"npm run server\" \"npm run timgolz-shopping-cart\"",
    "heroku-postbuild":      "NPM_CONFIG_PRODUCTION=false npm install --prefix timgolz-shopping-cart        && npm run build --prefix timgolz-shopping-cart",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "morgan": "^1.10.0"
  }
}
